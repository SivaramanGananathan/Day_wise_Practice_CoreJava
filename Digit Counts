import java.util.Scanner;

class digitsCount 
{
	public static void main(String[] args) 
	{
		Scanner scn = new Scanner(System.in);
		System.out.println("Enter the number: ");
		int num=scn.nextInt();
		int rem=0;
		int count=0;
		
		while(num>0)
		{
			rem = num%10;
			count++;
			num=num/10;
		}
		System.out.println(count);
	}
}
