import java.util.Scanner;

public class reverseTheDigits 
{
	public static void main(String[] args) 
	{
		Scanner scn = new Scanner(System.in);
		System.out.println("Enter the number: ");
		int num=scn.nextInt();
		int rem=0;
		int ans=0;
		
		while(num>0)
		{
			rem = num%10;
			ans = rem;
			System.out.print(ans);
			num=num/10;
		}
		
	}
}
